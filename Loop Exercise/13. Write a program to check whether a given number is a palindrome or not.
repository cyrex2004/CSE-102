#include <stdio.h>
int main()
{
    int k,p, n, num = 0;
    printf("Enter a Number: ");
    scanf("%d", &n);
    p=n;
    while (n != 0)
    {
        k = n % 10;
        num = num*10+k;
        n = n / 10;
    }
    if(p==num)
    {
        printf("Palindrome\n");
    }
    else
    {
        printf("Not a palindrome\n");
    }


    return 0;
}
